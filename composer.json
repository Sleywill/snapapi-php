{
    "name": "snapapi/sdk",
    "description": "Official PHP SDK for SnapAPI - Lightning-fast screenshot API",
    "version": "1.1.0",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "screenshot",
        "api",
        "webpage",
        "capture",
        "url-to-image",
        "web-screenshot",
        "snapapi"
    ],
    "authors": [
        {
            "name": "SnapAPI",
            "email": "support@snapapi.dev"
        }
    ],
    "homepage": "https://snapapi.pics",
    "support": {
        "issues": "https://github.com/Sleywill/snapapi-php/issues",
        "source": "https://github.com/Sleywill/snapapi-php"
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SnapAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SnapAPI\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse src"
    },
    "config": {
        "sort-packages": true
    }
}
